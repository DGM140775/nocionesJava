package ejemplo2;

public class Guerrero {
	
	private String nombreGuerrero;
	private Arma arma = new Espada();
	
	public String getNombreGuerrero() {
		return nombreGuerrero;
	}
	public void setNombreGuerrero(String nombreGuerrero) {
		this.nombreGuerrero = nombreGuerrero;
	}
			
	public Arma getArma() {
		return arma;
	}
	public void setArma(Arma arma) {
		this.arma = arma;
	}
	public void atacar() {
		System.out.println("Soy el guerrero " + nombreGuerrero + "y voy a atacar Argggggg!!!");
		
		
	}

}
